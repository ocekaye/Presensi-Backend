{
  "name": "Siswa",
  "base": "User",
  "strict": false,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    }
  },
  "properties": {
    "nama": {
      "type": "string"
    },
    "alamat": {
      "type": "string"
    },
    "tanggal_lahir": {
      "type": "string"
    },
    "tempat_lahir": {
      "type": "string"
    },
    "desc": {
      "type": "string"
    },
    "image": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "kelas": {
      "type": "hasAndBelongsToMany",
      "model": "Kelas",
      "foreignKey": ""
    },
    "nilais": {
      "type": "hasMany",
      "model": "Nilai",
      "foreignKey": ""
    },
    "presensis": {
      "type": "hasMany",
      "model": "Presensi",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
